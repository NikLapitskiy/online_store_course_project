  <script>
    // валидация цены товара
    document.getElementById('form-addProduct-btn').addEventListener('submit', function(e){
    const floatInput = document.getElementById('productPrice');
    floatInput.addEventListener('input', () => {
        const value = parseFloat(floatInput.value);
        if (isNaN(value)) {
            floatInput.value = "";
        } else {
            // floatInput.setCustomValidity('');
            floatInput.value = value;
        }
    });
})

<!-- Работа с товарами -->

<table class="table" id="viewProducts">
    <tr>
      <th>№ товара</th>
      <th>Название</th>
      <th>Цена</th>
      <th>№ типа</th>
      <th>№ бренда</th>
      <th>Заголовок</th>
      <th>Описание</th>
    </tr>
    <% if (products && products.length > 0) { %>
        <% products.forEach(({ id, name, price, typeId, brandId, product_info}) => { %>
          <tr>
          <td><%= id %></td>
          <td><%= name %></td>
          <td><%= price %></td>
          <td><%= typeId %></td>
          <td><%= brandId %></td>
          <% if(product_info) { %>
          <td><%= product_info.title %></td>
          <td><%= product_info.description %></td>
          <% } else { %>
            <td>Информации о товаре не найдено</td>
          <% } %>
          </tr>
          <% })} else { %>
            <td>Товаров не найдено!</td>
          <% } %>
  </table>
  <form action="/admin/addProduct" method="POST" class="admin-form" id="form-addProduct">
    <h3 class="admin-form-title">Добавление товара</h3>
    <div class="admin-form-input-box">
      <label for="productName" class="admin-form-input-box-label">Название товара:</label>
      <input type="text" name="name" id="productName" class="admin-form-input-box-input" placeholder="Введите навзание товара..." required>
    </div>
    <div class="admin-form-input-box">
      <label for="productPrice" class="admin-form-input-box-label">Цена товара:</label>
      <input type="text" name="price" id="productPrice" class="admin-form-input-box-input" placeholder="Введите цену товара..." required>
    </div>
    <div class="admin-form-input-box">
      <label for="productTypeId" class="admin-form-input-box-label">№ типа товара:</label>
      <input type="number" name="typeId" id="productTypeId" class="admin-form-input-box-input" min="0" max="100" placeholder="Введите № типа товара..." required>
    </div>
    <div class="admin-form-input-box">
      <label for="productBrandId" class="admin-form-input-box-label">№ бренда товара:</label>
      <input type="number" name="brandId" id="productBrandId" class="admin-form-input-box-input" min="0" max="100" placeholder="Введите № бренда товара..." required>
    </div>
    <div class="admin-form-input-box">
      <label for="productTitle" class="admin-form-input-box-label">Заголовок товара:</label>
      <input type="text" name="title" id="productTitle" class="admin-form-input-box-input" placeholder="Введите заголовок товара..." required>
    </div>
    <div class="admin-form-input-box">
      <label for="productDescription" class="admin-form-input-box-label">Описание товара:</label>
      <textarea name="description" id="productDescription" class="admin-form-input-box-input" rows="3" placeholder="Введите описание товара..." required></textarea>
    </div>
    <input type="submit" id="form-addProduct-btn" value="Добавить товар" class="admin-form-btn">
  </form>

